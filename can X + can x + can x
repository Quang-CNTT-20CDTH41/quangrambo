package CUOIKY1;
import java.util.Scanner;
public class bt3a {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n, x;
		System.out.printf("Nhap n: ");
		n = sc.nextInt();
		System.out.printf("Nhap x: ");
		x = sc.nextInt();
		double p = 0;
		for(int i=1; i<=n; i++) {
			p =(double) Math.sqrt(x+p);
		}
		System.out.printf("ket qua la: %f", p);
	}
}
