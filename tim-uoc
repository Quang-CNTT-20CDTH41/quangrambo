package CUOIKY1;
import java.util.Scanner;
public class ssss {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int s,n,m;
		System.out.printf("Nhap n: ");
		n = sc.nextInt();
		System.out.printf("uoc cua n: ");
		for(int i=1; i<=n; i++) {
			if(n%i==0) {
				System.out.printf(" %d",i);
			}
		}
	}
}
