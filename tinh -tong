package CUOIKY1;
import java.util.Scanner;
public class shhss {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		// tính tổng s = 1 + 1/2 + 1/3 + ... + 1/n
		int n, i;
		System.out.printf("Nhap n: ");
		n = sc.nextInt();
		float s = 0;
		for(i=1; i<=n; i++) {
			s +=(float)1/i;
		}
		System.out.printf("ket qua la: %f",s);
	}
}
